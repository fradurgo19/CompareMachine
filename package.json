{
  "name": "compare-machine-unified",
  "version": "1.0.0",
  "type": "module",
  "description": "CompareMachine - Frontend y Backend unificados",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend-unified && npm run dev",
    "build": "vite build",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend-unified && npm run build",
    "install:all": "npm install && cd frontend && npm install && cd ../backend-unified && npm install"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "axios": "^1.7.7",
    "chart.js": "^4.4.6",
    "react-chartjs-2": "^5.2.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.2",
    "react-hook-form": "^7.53.2",
    "react-hot-toast": "^2.4.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.468.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "vite": "^5.4.8",
    "@vitejs/plugin-react": "^4.3.1",
    "typescript": "^5.9.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "tailwindcss": "^3.4.15",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "terser": "^5.36.0"
  }
}
